{
  "name": "pb_cypress_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --config watchForFileChanges=false",
    "cy:run": "npx cypress run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/report/mochawesome-report/*.json > cypress/report/output.json",
    "generate-report": "marge cypress/report/output.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "testmocha" : "npm run scripts || npm run posttest"



    

